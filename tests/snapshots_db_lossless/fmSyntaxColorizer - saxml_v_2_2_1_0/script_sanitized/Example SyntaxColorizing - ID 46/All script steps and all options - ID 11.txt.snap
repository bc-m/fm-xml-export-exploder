# Note: This script is used to check the output of fmCheckMates Print function

# 20170516 MrW Script sorted alphabetically within categories for fm16

# Calculation
Variable setzen [ $MBS_Command_Results ; SetzeVar([ 
				$Command_EN =_Syntax::Command_EN ;
				$Command_Foreign = Syntax.GUISyntax::Command ;
				$CommandColoringOn = _Syntax::On = 1;
				$ColorExists = Syntax.Color::__ID > 0 UND Syntax.Color::ColorName ≠ "--None--";
				$ColorHex = Syntax.Color::_kColorHex ;
				vStamp = Hole( SystemUhrzeit )
				];
				Falls( 
				
				_Syntax::_kIsGroupTitle ; "" ;
				
				NICHT $CommandColoringOn ODER NICHT $ColorExists ;
				vStamp & " Removed: English " & MBS( "SyntaxColoring.RemoveTag" ; $Command_EN ) & 
				Falls( _Syntax::_gLanguage <> "English" UND NICHT IstLeer( $Command_Foreign ) ;
				  ", " & _Syntax::_gLanguage & " " & MBS( "SyntaxColoring.RemoveTag" ; $Command_Foreign )
				) ;
				
				$CommandColoringOn UND $ColorExists ;
				vStamp & " Added: English " & MBS( "SyntaxColoring.AddTag" ; $Command_EN ; $ColorHex ) & 
				Falls( _Syntax::_gLanguage ≠ "English" UND NICHT IstLeer( $Command_Foreign ) ;
				  ", " & _Syntax::_gLanguage & " " & MBS( "SyntaxColoring.AddTag" ; $Command_Foreign ; $ColorHex )
				) ;
				
				"-"
				
				)) ]

